	<solution id="eip-499">
		<para id="eip-219">24</para>
	</solution>
</exercise><exercise id="eip-828"><problem id="eip-763">
		<para id="eip-65">
    <m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>1</m:mn><m:mo>+</m:mo><m:mn>19</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mn>2</m:mn><m:mo>+</m:mo><m:mn>3</m:mn>
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>

  </para>
	</problem>
	<solution id="eip-125">
		<para id="eip-175">4</para>
	</solution>
</exercise><para id="eip-935">The following examples show how to use algebraic notation to write each expression.</para><example id="eip-226"><para id="eip-747">9 minus <m:math>
		<m:semantics>
			<m:mi>y</m:mi>
			<m:annotation encoding="MathType-MTEF">
			</m:annotation>
		</m:semantics>
	</m:math> becomes <m:math>
		<m:semantics>
			<m:mrow>
				<m:mn>9</m:mn><m:mo>−</m:mo><m:mi>y</m:mi>
			</m:mrow>
			<m:annotation encoding="MathType-MTEF">
			</m:annotation>
		</m:semantics>
	</m:math>
</para>
</example><example id="eip-290"><para id="eip-530">
46 times <m:math>
			<m:semantics>
				<m:mi>x</m:mi>
				<m:annotation encoding="MathType-MTEF">
				</m:annotation>
			</m:semantics>
		</m:math> becomes <m:math>
			<m:semantics>
				<m:mrow>
					<m:mi>46x</m:mi>
				</m:mrow>
				<m:annotation encoding="MathType-MTEF">
				</m:annotation>
			</m:semantics>
		</m:math>
</para>
</example><example id="eip-475"><para id="eip-751">
 7 times <m:math>
			<m:semantics>
				<m:mrow>
					<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo stretchy="false">)</m:mo>
				</m:mrow>
				<m:annotation encoding="MathType-MTEF">
				</m:annotation>
			</m:semantics>
		</m:math> becomes <m:math>
			<m:semantics>
				<m:mrow>
					<m:mn>7</m:mn><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo stretchy="false">)</m:mo>
				</m:mrow>
