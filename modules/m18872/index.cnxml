							<m:mrow>
								<m:mn>16</m:mn><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>⋅</m:mo><m:mn>9</m:mn>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mtext>Multiply</m:mtext><m:mtext> </m:mtext><m:mtext>first</m:mtext><m:mtext>.</m:mtext></m:mrow>
						</m:mtd>
					</m:mtr>
