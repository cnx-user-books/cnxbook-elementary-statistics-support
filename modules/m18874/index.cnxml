<exercise id="eip-565">
<problem id="eip-71">		<para id="eip-678">
<m:math> <m:semantics>  <m:mrow>   <m:mo>−</m:mo>
<m:mn>15</m:mn>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics>
</m:math>  </para>	</problem>	<solution id="eip-701">		<para id="eip-468">15</para>	</solution>
</exercise>
<exercise id="eip-22">
<problem id="eip-523">		<para id="eip-987">
<m:math> <m:semantics>  <m:mrow>   <m:mo>−</m:mo>
<m:mo stretchy="false">(</m:mo>
<m:mo>−</m:mo>
<m:mn>1</m:mn>
<m:mo stretchy="false">)</m:mo>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics>
</m:math>  </para>	</problem>	<solution id="eip-855">		<para id="eip-63">
<m:math> <m:semantics>  <m:mrow>   <m:mo>−</m:mo>
<m:mn>1</m:mn>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics>
</m:math>, since <m:math> <m:semantics>  <m:mrow>   <m:mo>−</m:mo>
<m:mo stretchy="false">(</m:mo>
<m:mo>−</m:mo>
<m:mn>1</m:mn>
<m:mo stretchy="false">)</m:mo>
<m:mo>=</m:mo>
